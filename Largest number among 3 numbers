#include<stdio.h>
int main()
{
    int num1,num2,num3,l;
    printf("Enter first number:");
    scanf("%d", &num1);
    
    printf("Enter second number:");
    scanf("%d", &num2);
    
    printf("Enter third number:");
    scanf("%d", &num3);
    
        if(num1>num2)
        {
            printf("num1 %d is greater than num2 %d \n",num1,num2);
            l=num1;
        }
        else
        {
            printf("num2 %d is greater than num1 %d \n",num1,num2);
            l=num2;
        }
        if(l>num3)
        {
            printf("l %d is largest number \n",l);
        }
        else
        {
            printf("num3 is largest number \n",num3);
        }
        return 0;
    
}
